pipeline {

agent none

stages {

stage('Build') {
   agent {
                label 'jk-slave1'
            }
steps {echo 'Building..'}

}

stage('Test') {
  agent {
                label 'jk-slave1'
            }
steps {echo 'Testing..'}

}

stage('Deploy') {
agent {
                label 'jk-slave1'
            }

steps {echo 'Deploying....'}

}

}

}

